    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "KUBECONFIG": "${workspaceFolder}/kubeconfig",
                "CRATER_DEBUG_CONFIG_PATH": "${workspaceFolder}/etc/debug-config.yaml",
                //"AIJOB_UNPLUGIN": "true",
                //"SPJOB_UNPLUGIN": "true",
            }
        },
        {
            "name": "ACT GPU Server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "KUBECONFIG": "${workspaceFolder}/kubeconfig_act",
                "CRATER_DEBUG_CONFIG_PATH": "${workspaceFolder}/etc/debug-config-actgpu.yaml",
                //"AIJOB_UNPLUGIN": "true",
                //"SPJOB_UNPLUGIN": "true",
            }
        },
    ]
}