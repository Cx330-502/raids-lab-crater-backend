    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "KUBECONFIG": "${workspaceFolder}/kubeconfig",
                //"AIJOB_UNPLUGIN": "true",
                //"SPJOB_UNPLUGIN": "true",
            },
            "args": [
                "--config-file",
                "./etc/debug-config.yaml",
                "--metrics-bind-address",
                ":8097",
                "--health-probe-bind-address",
                ":8096",
                "--server-port",
                ":8099"
            ]
        }
    ]
}